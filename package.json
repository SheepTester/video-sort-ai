{
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "esbuild": "^0.25.8",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "scripts": {
    "dev": "npm-run-all --parallel serve client:dev",
    "serve": "cargo run",
    "build": "npm run client:build",
    "client:build": "esbuild --bundle client/index.tsx --outdir=src/static/ --define:ROOT='window.location.href' --minify",
    "client:dev": "esbuild --bundle client/index.tsx --outdir=src/static/ --define:ROOT='\"http://localhost:8008/\"' --servedir=src/static/ --sourcemap"
  }
}
